{
  "name": "umbriel-checkout",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Thiago Watanabe <watanabe.thiago4@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsup src --out-dir build",
    "dev": "ts-node-dev --inspect -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules src/shared/infra/http/server.ts",
    "start:dev": "ts-node -r tsconfig-paths/register --transpile-only src/shared/infra/http/server.ts",
    "typesense": "docker start PandorisTypesense",
    "start": "node build/shared/infra/http/server.js",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
    "migration:generate": "npm run typeorm migration:generate ./src/shared/infra/typeorm/migrations -- -n",
    "migration:run": "npx typeorm-ts-node-esm migration:run --dataSource ./src/shared/infra/typeorm/index.ts",
    "migration:create": "typeorm migration:create ./migrations/migrate",
    "migrate": "typeorm migration:create -n",
    "test": "jest --setupFiles dotenv/config",
    "test:u": "jest",
    "seed:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js seed",
    "collections": "node --unhandled-rejections=strict src/shared/infra/typesense/collections.js"
  },"resolutions": {
    "wrap-ansi": "7.0.0",
    "string-width": "4.1.0"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "add": "^2.0.6",
    "amqplib": "^0.10.3",
    "axios": "^1.1.3",
    "cfonts": "^3.1.1",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "glob": "^8.1.0",
    "google-libphonenumber": "^3.2.33",
    "helmet": "^6.0.0",
    "http-terminator": "^3.2.0",
    "kafkajs": "^2.2.2",
    "mssql": "^9.3.2",
    "nice-cloud-config-client": "^1.0.1",
    "pino": "^8.6.1",
    "pino-http": "^8.2.1",
    "prisma": "^5.0.0",
    "prom-client": "^14.1.0",
    "typeorm": "^0.3.17",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@vitest/coverage-c8": "^0.24.3",
    "concurrently": "^7.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.2",
    "pino-pretty": "^9.1.1",
    "plop": "^3.1.1",
    "vitest": "^0.24.3"
  }
}



