{
  "name": "umbriel-checkout",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Thiago Watanabe <watanabe.thiago4@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsup src --out-dir build",
    "dev": "ts-node-dev --inspect -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules src/shared/infra/http/server.ts",
    "start:dev": "ts-node -r tsconfig-paths/register --transpile-only src/shared/infra/http/server.ts",
    "typesense": "docker start PandorisTypesense",
    "start": "node build/shared/infra/http/server.js",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
    "migration:generate": "npm run typeorm migration:generate ./src/shared/infra/typeorm/migrations -- -n",
    "migration:run": "npx typeorm-ts-node-esm migration:run --dataSource ./src/shared/infra/typeorm/index.ts",
    "migration:create": "typeorm migration:create ./migrations/migrate",
    "migrate": "typeorm migration:create -n",
    "test": "jest --setupFiles dotenv/config",
    "test:u": "jest",
    "seed:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js seed",
    "collections": "node --unhandled-rejections=strict src/shared/infra/typesense/collections.js"
  },"resolutions": {
    "wrap-ansi": "7.0.0",
    "string-width": "4.1.0"
  },
  "dependencies": {
    "@fastify/cors": "8.3.0",
    "@fastify/express": "^2.3.0",
    "@fastify/jwt": "^5.0.1",
    "@fastify/multipart": "^7.7.3",
    "@hapi/joi": "^17.1.1",
    "@prisma/client": "^4.16.2",
    "@types/numeral": "^2.0.1",
    "axios": "^0.24.0",
    "pino": "8.6.1",
    "class-transformer": "^0.5.1",
    "cors": "^2.8.5",
    "date-fns": "^2.22.1",
    "dotenv": "^8.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "fastify": "^4.21.0",
    "fastify-file-upload": "^4.0.0",
    "fastify-multer": "^2.0.3",
    "fastify-plugin": "^4.5.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "nanoid": "^5.0.4",
    "os-utils": "^0.0.14",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.13",
    "tap": "^18.7.0",
    "ts-node": "^10.9.1",
    "tsup": "^8.0.1",
    "tsyringe": "^4.3.0",
    "typeorm": "^0.3.17",
    "typescript": "4.9.4",
    "typesense": "^1.7.2",
    "winston": "^3.3.3",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.8",
    "@types/date-fns": "^2.6.0",
    "@types/fastify-cors": "^2.1.0",
    "@types/jest": "^26.0.22",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.10",
    "@types/os-utils": "^0.0.1",
    "@types/uuid": "^9.0.7",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.1",
    "jest": "^26.6.3",
    "prettier": "^3.2.5",
    "prisma": "^4.16.2",
    "ts-jest": "^26.5.3",
    "ts-node-dev": "^2.0.0"
  }
}
